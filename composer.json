{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "friendsofsymfony/user-bundle": "~2.0",
        "knplabs/knp-menu-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^3.0",
        "kriswallsmith/buzz": "^1.0",
        "nyholm/psr7": "^1.1",
        "php-http/guzzle6-adapter": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "spatie/url": "^1.3",
        "stof/doctrine-extensions-bundle": "^1.2",
        "sylius/resource-bundle": "^1.6",
        "symfony/asset": "4.2.*",
        "symfony/console": "4.2.*",
        "symfony/dotenv": "4.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/webpack-encore-bundle": "^1.5",
        "symfony/yaml": "4.2.*",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "twig/extensions": "^1.5",
        
        "iatanasov77/vs-core-bundle": "dev-develop",
        "iatanasov77/vs-users-bundle": "dev-develop",
		"iatanasov77/vs-payment-bundle": "dev-develop",
       	"iatanasov77/vs-cms-bundle": "dev-develop"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
	"repositories": [
		{
            "type": "git",
            "url": "https://github.com/iatanasov77/vs-core-bundle.git"
        },
		{
            "type": "git",
            "url": "https://github.com/iatanasov77/vs-users-bundle.git"
        },
        {
            "type": "git",
            "url": "https://github.com/iatanasov77/vs-cms-bundle.git"
        },
        {
            "type": "git",
            "url": "https://github.com/iatanasov77/vs-payment-bundle.git"
        }
	],
    "config": {
        "process-timeout":      600,
        "preferred-install":    "dist",
        "github-protocols":     ["https"]
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.2.*"
        }
    }
}
